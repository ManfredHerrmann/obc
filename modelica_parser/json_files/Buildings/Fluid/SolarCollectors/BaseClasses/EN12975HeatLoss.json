{"within":[{"name":"Buildings.Fluid.SolarCollectors.BaseClasses"}],"class_definition":[{"class_prefixes":{"prefix":"block"},"class_specifier":{"long_class_specifier":{"name":"EN12975HeatLoss","comment":{"string":"\"Calculate the heat loss of a solar collector per EN12975\""},"composition":{"protect":["protected"],"element_list":{"element":[{"extends_clause":{"name":{"name":"Buildings.Fluid.SolarCollectors.BaseClasses.PartialHeatLoss"}}},{"component_clause":{"type_prefix":{"prefix":" parameter"},"type_specifier":{"specifier":{"name":"Modelica.SIunits.CoefficientOfHeatTransfer"}},"component_list":{"component_declaration":[{"declaration":{"name":"C1"},"comment":{"comment":{"string":"\"C1 from ratings data\""}}}]}}},{"component_clause":{"type_prefix":{"prefix":" parameter"},"type_specifier":{"specifier":{"name":"Real"}},"component_list":{"component_declaration":[{"declaration":{"name":"C2","modification":{"class_modification":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"prefix":"final","element_modification":{"name":{"name":"unit"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"\"W/(m2.K2)\""}]}}]}}}}}}}},"comment":{}}}}]}}}},"comment":{"comment":{"string":"\"C2 from ratings data\""}}}]}}}]},"prefixed_element":[{"element":[{"prefix":"final ","component_clause":{"type_prefix":{"prefix":" parameter"},"type_specifier":{"specifier":{"name":"Modelica.SIunits.ThermalConductance"}},"component_list":{"component_declaration":[{"declaration":{"name":"UA","modification":{"class_modification":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"element_modification":{"name":{"name":"fixed"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"false"}]}
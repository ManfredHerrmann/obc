{"within":[{"name":"Buildings.Fluid.HeatExchangers.ActiveBeams.Data.BaseClasses"}],"class_definition":[{"class_prefixes":{"prefix":"null record"},"class_specifier":{"long_class_specifier":{"name":"TemperatureDifference","comment":{"string":"\"Record for temperature difference\""},"composition":{"element_list":{"element":[{"extends_clause":{"name":{"name":"Modelica.Icons.Record"}}},{"component_clause":{"type_prefix":{"prefix":" parameter"},"type_specifier":{"specifier":{"name":"Real"}},"component_list":{"component_declaration":[{"declaration":{"name":"r_dT","array_subscripts":{"subscript":[{"subscript_colon":":"}]},"modification":{"class_modification":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"prefix":"each ","element_modification":{"name":{"name":"min"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"0"}]}}]}}}}}}}},"comment":{}}}},{"element_modification_or_replaceable":{"prefix":"each final","element_modification":{"name":{"name":"unit"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"\"1\""}]}}]}}}}}}}},"comment":{}}}}]}}}},"comment":{"comment":{"string":"\"Normalized temperature difference, e.g., temperature difference at\n user-selected operating points divided by nominal temperature difference.\n Must be positive.\""}}}]}}},{"component_clause":{"type_prefix":{"prefix":" parameter"},"type_specifier":{"specifier":{"name":"Real"}},"component_list":{"component_declaration":[{"declaration":{"name":"f","array_subscripts":{"subscript":[{"subscript":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"name":{"name":"size"},"function_call_args":{"arguments":{"argument":{"expressions":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"component_reference":{"component":"r_dT"}}]}}]}}}}}}}},"arguments":{"argument":{"expressions":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"1"}]}}]}}}}}}}}}}}}]}}]}}}}}}}}]},"modification":{"class_modification":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"prefix":"each ","element_modification":{"name":{"name":"min"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"0"}]}}]}}}}}}}},"comment":{}}}},{"element_modification_or_replaceable":{"prefix":"each final","element_modification":{"name":{"name":"unit"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"\"1\""}]}}]}}}}}}}},"comment":{}}}}]}}}},"comment":{"comment":{"string":"\"Normalized performance factor at these normalized temperature differences\""}}}]}}}]},"comp_annotation":{"prefix":"annotation","annotation":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"element_modification":{"name":{"name":"Documentation"},"modification":{"class_modification":{"argument_list":{"argument":[{"element_modification_or_replaceable":{"element_modification":{"name":{"name":"info"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"\"\u003chtml\u003e\n\u003cp\u003e\nData record for performance data that describe the normalized\ntemperature difference\nversus the change in the rate of heating or cooling.\nThe normalized temperature difference is defined as\n\u003c/p\u003e\n\u003cp align\u003d\\\"center\\\" style\u003d\\\"font-style:italic;\\\"\u003e\nr\u003csub\u003e\u0026#916;T\u003c/sub\u003e\u003csup\u003ei\u003c/sup\u003e\u003d\n\u0026#916;T\u003csup\u003ei\u003c/sup\u003e \u0026frasl; \u0026#916;T\u003csub\u003enominal\u003c/sub\u003e\n\u003d\n(T\u003csub\u003ew\u003c/sub\u003e\u003csup\u003ei\u003c/sup\u003e-T\u003csub\u003ez\u003c/sub\u003e)\n\u0026frasl;\n(T\u003csub\u003ew,nominal\u003c/sub\u003e-T\u003csub\u003ez\u003c/sub\u003e),\n\u003c/p\u003e\n\u003cp\u003e\nwhere\n\u003ci\u003eT\u003csub\u003ew\u003c/sub\u003e\u003csup\u003ei\u003c/sup\u003e\u003c/i\u003e is the water inlet temperature,\n\u003ci\u003eT\u003csub\u003ez\u003c/sub\u003e\u003c/i\u003e is the zone air temperature and\n\u003ci\u003eT\u003csub\u003ew,nominal\u003c/sub\u003e\u003c/i\u003e is the nominal water inlet temperature.\n\u003c/p\u003e\n\u003cp\u003e\nThe normalized temperature difference \u003ci\u003er\u003csub\u003e\u0026#916;T\u003c/sub\u003e\u003c/i\u003e must be strictly increasing, i.e.,\n\u003ci\u003er\u003csub\u003e\u0026#916;T\u003c/sub\u003e\u003csup\u003ei\u003c/sup\u003e \u0026lt; r\u003csub\u003e\u0026#916;T\u003c/sub\u003e\u003csup\u003ei+1\u003c/sup\u003e\u003c/i\u003e.\nBoth vectors, \u003ci\u003er\u003csub\u003e\u0026#916;T\u003c/sub\u003e\u003c/i\u003e and \u003ci\u003ef\u003c/i\u003e\nmust have the same size.\n\u003c/p\u003e\n\u003c/html\u003e\""}]}}]}}}}}}}},"comment":{}}}},{"element_modification_or_replaceable":{"element_modification":{"name":{"name":"revisions"},"modification":{"symbol":"\u003d","expression":{"simple_expression":{"expression1":{"logical_term":{"logical_factor":{"relation":{"expression1":{"terms":[{"factor":{"primarys":[{"simple_primary":"\"\u003chtml\u003e\n\u003cul\u003e\n\u003cli\u003e\nJune 13, 2016, by Michael Wetter:\u003cbr/\u003e\nRevised implementation.\n\u003c/li\u003e\n\u003cli\u003e\nMay 20, 2016, by Alessandro Maccarini:\u003cbr/\u003e\nFirst implementation.\n\u003c/li\u003e\n\u003c/ul\u003e\n\u003c/html\u003e\""}]}}]}}}}}}}},"comment":{}}}}]}}},"comment":{}}}}]}}}}}}}]}